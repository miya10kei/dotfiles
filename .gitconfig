[alias]
  ad = add
	br = branch
	cf = config
	ch = checkout
	cm = commit
	df = diff
  graph = log --graph --date-order --all --pretty=format:'%h %Cred%d %Cgreen%ad %Cblue%cn %Creset%s' --date=short
	pl = pl
	ps = push
	st = status
[color]
	branch = auto
	diff = auto
	status = auto
	ui = auto
[core]
	editor = vim -c \"set fenc=utf-8\"
	excludesfile = ~/.gitignore_global
	quotepath = false
	ignorecase = false
[push]
	default = simple
[user]
	email = miya10kei@gmail.com
	name = miya10kei
