{
  "languageserver": {
    "bash": {
      "command": "bash-language-server",
      "args": [
        "start"
      ],
      "filetypes": [
        "sh"
      ],
      "ignoredRootPaths": [
        "~"
      ]
    }
  },
  "vetur.format.defaultFormatter.js": "prettier-eslint",
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": [
    "javascript",
    "typescript"
  ]
}
